{
  "name": "waqarraza/laravel-backend-utilities",
  "description": "Laravel backend utilities, helpers and more",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "utilities",
    "laravel utilities",
    "laravel-utilities",
    "util"
  ],
  "autoload": {
    "psr-4": {
      "Waqar\\Utility\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Muhammad Waqar",
      "email": "waqarqadri6@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/framework": ">=6"
  },
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/WaqarRaza/laravel-backend-utilities/"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Waqar\\Utility\\Provider\\ApiServiceProvider",
        "Waqar\\Utility\\Provider\\UtilityServiceProvider"
      ],
      "aliases": {
        "Utility": "Utility",
        "ApiService": "ApiService"
      }
    }
  }
}
